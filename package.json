{
	"name": "pixiv-cli",
	"version": "1.0.2",
	"description": "CLI tool for pixiv",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc && npm run minify",
		"minify": "terser dist/index.js --compress --mangle --output dist/index.js"
	},
	"files": [
		"dist/**/*"
	],
	"bin": {
		"pixiv": "dist/index.js"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FireStreaker2/pixiv-cli.git"
	},
	"keywords": [
		"pixiv",
		"cli"
	],
	"author": "FireStreaker2",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/FireStreaker2/pixiv-cli/issues"
	},
	"homepage": "https://github.com/FireStreaker2/pixiv-cli#readme",
	"dependencies": {
		"commander": "^12.1.0",
		"pixiv-node": "^1.0.7"
	},
	"devDependencies": {
		"@types/node": "^20.14.10",
		"terser": "^5.31.2",
		"typescript": "^5.5.3"
	}
}
